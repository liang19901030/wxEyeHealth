/* pages/myFamily/myFamily.wxss */

label {
  overflow: hidden;
  text-overflow: ellipsis;
}

.container {
  display: flex;
}

.myFamily-scroll {
  background-color: #f4f4f4;
}

.myFamily-list {
  display: flex;
  margin-top: 20rpx;
  width: 100%;
  height: 120rpx;
  background-color: white;
  align-items: center;
}

.basicInfo {
  display: flex;
  align-items: center;
  width: 45%;
}

.number {
  display: flex;
}

.name {
  display: flex;
  margin-left: 20rpx;
  width: 200rpx;
  color: #191919;
  font-size: 17px;
  align-items: center;
}

.sex {
  width: 26rpx;
  height: 26rpx;
  margin-left: 10rpx;
}

.age {
  width: 100rpx;
  margin-left: 13rpx;
  color: #4D4D4D;
  font-size: 13px;
}