<!--pages/myOrder/myOrder.wxml-->
<view class='container'>
  <view class='myOrder'>
    <view class='tabs'>
      <label class='tabs-lab'>待就诊预约</label>
      <label class='not-treatment-line'></label>
    </view>
    <view class='tabs'>
      <label class='tabs-lab'>历史预约</label>
      <label class='history-order-line'></label>
    </view>
  </view>

  <view class='my-order-list-container' wx:if="{{myOrderList.length > 0}}">
    <scroll-view class='my-order-scroll' scroll-y>
      <view class='my-order-list' wx:for="{{myOrderList}}" wx:for-item="order">
        <image class='doctor-header' src='../../img/avater.png'></image>
        <view class='info'>
          ss
        </view>
      </view>
    </scroll-view>
  </view>
</view>